# LYNX Environment Configuration

# Application
NODE_ENV=development
PORT=3001
FRONTEND_URL=http://localhost:3000

# Microsoft OneLake/Fabric Configuration
ONELAKE_CONNECTION_STRING=your_onelake_connection_string
FABRIC_WORKSPACE_ID=your_fabric_workspace_id
FABRIC_DATASET_ID=your_fabric_dataset_id

# Blockchain API Keys
ETHERSCAN_API_KEY=your_etherscan_api_key
BLOCKCYPHER_API_KEY=your_blockcypher_api_key
BSCSCAN_API_KEY=your_bscscan_api_key
POLYGONSCAN_API_KEY=your_polygonscan_api_key

# AI/ML Configuration
AI_MODEL_ENDPOINT=your_ai_model_endpoint
AI_API_KEY=your_ai_api_key

# Security
JWT_SECRET=your_jwt_secret_key
ENCRYPTION_KEY=your_encryption_key

# Logging
LOG_LEVEL=info

# Rate Limiting
RATE_LIMIT_WINDOW_MS=900000
RATE_LIMIT_MAX_REQUESTS=100

# Database (if using additional database)
DATABASE_URL=your_database_url

# Redis (for caching)
REDIS_URL=your_redis_url

# Monitoring
SENTRY_DSN=your_sentry_dsn 